{
  "name": "tiny-rag",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "init": "node ./init_env.js",
    "deploy:app": "pnpm deploy --filter=tiny-rag-app --legacy ./packages/app/node_modules/",
    "deploy:server": "pnpm deploy --filter=tiny-rag-server --legacy ./packages/server/node_modules/",
    "run:nobuild": "pnpm run deploy:app && pnpm run deploy:server && docker compose -f ./docker-compose.nobuild.yml up -d",
    "run:build": "docker compose up -d",
    "stop": "docker compose down"
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "axios": "^1.13.2",
    "lodash": "^4.17.21"
  },
  "license": "ISC",
  "packageManager": "pnpm@10.24.0",
  "devDependencies": {
    "fs-extra": "^11.3.2",
    "inquirer": "^13.1.0",
    "typescript": "^5.9.3"
  }
}
